#include<stdio.h>
main()
{
  int a=21;
  int c;
  
  c=a;
  printf("Line - = 运算符实例，c的值 = %d\n",c);
  
  c+=a;
  printf("Line - += 运算符实例，c的值 = %d\n",c);
  
  c-=a;
  printf("Line - -= 运算符实例，c的值 = %d\n",c);
  
  c*=a;
  printf("Line - *= 运算符实例，c的值 = %d\n",c);
  
  c/=a;
  printf("Line - /= 运算符实例，c的值 = &d\n",c);
  
  c=200;
  c%=a;
  printf("Line - %= 运算符实例，c的值 = &d\n",c);
  
  c<<=2;
  printf("Line - <<= 运算符实例，c的值 = &d\n",c);
}
